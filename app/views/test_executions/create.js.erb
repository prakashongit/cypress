<% if params['partial'] == '/products/product_test_link' %>
  <% # set product for /products/measure_tests_table partial %>
  <% @product = @test_execution.task.product_test.product %>
  <% # set PATH_INFO so AJAX call can be made in /products/measure_tests_table partial. this AJAX call is for reloading each product_test_link %>
  <% request.env['PATH_INFO'] = vendor_product_path(@product.vendor, @product) %>
  $.ajax({url: "<%= request.env['PATH_INFO'] %>", type: "GET", dataType: 'script', data: { "partial": "<%= params['partial'] %>", task_id: "<%= params[:task_id] %>" }});

<% elsif params['partial'] == '/test_executions/results/execution_results_panel' %>
  $.ajax({url: "<%= task_test_execution_path(@test_execution.task, @test_execution) %>", type: "GET", dataType: 'script', data: { "partial": "<%= params['partial'] %>", task_id: "<%= params[:task_id] %>" }});
<% end %>
